{
  "name": "beeai-framework",
  "version": "0.1.12",
  "license": "Apache-2.0",
  "description": "BeeAI Framework - LLM Agent Framework",
  "author": "BeeAI a Series of LF Projects, LLC",
  "contributors": [
    "Tomas Dvorak <tomas.dvorak@ibm.com>"
  ],
  "keywords": [
    "BeeAI Framework",
    "LLM Agent Framework",
    "Bee Agent Framework",
    "NodeJS Agent Framework"
  ],
  "packageManager": "pnpm@4.1.1",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "signed-off-by": [
        2,
        "always",
        "Signed-off-by"
      ],
      "scope-enum": [
        2,
        "always",
        [
          "adapters",
          "agents",
          "backend",
          "tools",
          "cache",
          "emitter",
          "internals",
          "logger",
          "memory",
          "serializer",
          "infra",
          "deps",
          "instrumentation",
          "workflows"
        ]
      ]
    }
  },
  "lint-staged": {
    "{src,tests,scripts}/**/*": "pnpm copyright",
    "*": "prettier --ignore-unknown --write",
    "*.{ts,js}": "eslint --fix"
  },
  "prettier": {
    "printWidth": 100,
    "quoteProps": "consistent"
  },
  "release-it": {
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": {
          "name": "conventionalcommits"
        },
        "header": "# Changelog",
        "infile": "CHANGELOG.md",
        "gitRawCommitsOpts": {
          "path": "."
        },
        "commitsOpts": {
          "path": "."
        }
      }
    },
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "typescript_v${version}",
      "commitArgs": [
        "-s",
        "--no-verify"
      ]
    },

    "hooks": {
      "before:init": [
        "pnpm docs:check",
        "[ \"$RELEASE_SKIP_DOCS_LINKS\" = \"true\" ] && echo 'Skipping docs links check.' || pnpm docs:links:check",
        "pnpm lint",
        "pnpm ts:check",
        "[ \"$RELEASE_SKIP_TESTS\" = \"true\" ] && echo 'Skipping tests.' || pnpm test:all"
      ],
      "after:bump": [
        "pnpm build"
      ],
      "before:release": [
        "pnpm docs:build && git add . --update",
        "pnpm lint:fix && pnpm format:fix && git add . --update"
      ]
    },
    "github": {
      "release": true,
      "releaseName": "typescript_v${version}"
    }
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./*": {
      "import": {
        "types": "./dist/*.d.ts",
        "default": "./dist/*.js"
      },
      "require": {
        "types": "./dist/*.d.cts",
        "default": "./dist/*.cjs"
      }
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*.d.ts",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist/**/*"
  ],
  "homepage": "https://github.com/i-am-bee/beeai-framework#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/i-am-bee/beeai-framework.git"
  },
  "bugs": {
    "url": "https://github.com/i-am-bee/beeai-framework/issues"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "pnpm clean && pnpm ts:check && NODE_OPTIONS='--max-old-space-size=8192' tsup",
    "ts:check": "tsc --noEmit && tsc -p tsconfig.examples.json --noEmit",
    "start": "tsx --tsconfig tsconfig.examples.json",
    "start:bee": "pnpm start -- examples/agents/bee.ts",
    "docs:build": "embedme --source-root=. docs/**/*.md && pnpm lint:fix docs/ && pnpm prettier --write docs/",
    "docs:check": "embedme --source-root=. docs/**/*.md --verify",
    "docs:links": "linkinator . --timeout=10000 --retry --markdown --directory-listing --skip node_modules --skip CHANGELOG.md --skip docs/CHANGELOG.md --skip docs/_sidebar.md --skip \"https://ai.meta.com/blog/meta-llama-3-1/\" --include",
    "docs:links:check": "pnpm docs:links docs/**/*.md",
    "docs:watch": "docsify serve ./docs --open",
    "lint": "pnpm eslint",
    "prepare": "../.githooks/install.sh",
    "lint:fix": "pnpm eslint --fix",
    "format": "pnpm prettier --check .",
    "format:fix": "pnpm prettier --write .",
    "test:unit": "vitest run src",
    "test:unit:watch": "vitest run src",
    "test:e2e": "vitest run tests/e2e",
    "test:e2e:watch": "vitest watch tests/e2e",
    "test:all": "pnpm test:unit && pnpm test:e2e && pnpm test:examples",
    "test:examples": "vitest --config ./examples/vitest.examples.config.ts run tests/examples",
    "test:examples:watch": "vitest --config ./examples/vitest.examples.config.ts watch tests/examples",
    "copyright": "./scripts/copyright.sh",
    "copyright:check": "TYPE=check ./scripts/copyright.sh",
    "release": "release-it",
    "_ensure_env": "cp -n .env.template .env || true",
    "git:commit-msg": "pnpm commitlint --edit --",
    "git:pre-commit": "pnpm copyright:check && pnpm docs:check && pnpm lint && pnpm format && pnpm ts:check && pnpm run test:unit"
  },
